    {
      "id": "WGMFAHOxaRmslle5YKuck",
      "type": "text",
      "x": 1227.7534814528449,
      "y": 226.7550134604688,
      "width": 1905.8236083984375,
      "height": 1036.8000000000002,
      "angle": 0,
      "strokeColor": "#1e1e1e",
      "backgroundColor": "#fcc2d7",
      "fillStyle": "hachure",
      "strokeWidth": 1,
      "strokeStyle": "solid",
      "roughness": 0,
